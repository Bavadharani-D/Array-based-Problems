import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
      int size = sc.nextInt();
      int arr[] = new int[size];
      for(int i= 0;i<size;arr[i]=sc.nextInt(),i++);
      int max=arr[0];
      for(int i=1;i<size;i++){
        max = arr[i];
      }
      System.out.println(max);
    }}
      
